{% if data.threeCallOutWithLink %}
    <div class="threeWithLink">

        <div class="rows flex between">
            {% for i in data.threeCallOuts[0].callouts %}
                <div class="callout">
                    <p>{{ i.text }}</p>
                </div>
            {% endfor %}
        </div>

        <a href="{{data.threeCallOuts[0].linkAddress}}" target="_blank">{{data.threeCallOuts[0].linkText}}</a>
           
    </div>
{% endif %} 